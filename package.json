{
  "name": "fintel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run clean && npm run start",
    "start": "concurrently --kill-others --names \"backend,frontend\" --prefix-colors \"blue,green\" \"npm run start:backend\" \"npm run wait-for-backend && npm run start:frontend\"",
    "start:backend": "cd backend && source venv/bin/activate && export PYTHONPATH=$PYTHONPATH:$(dirname $(pwd)) && BACKEND_PORT=5001 python3 app.py",
    "start:frontend": "vite --host 0.0.0.0 --port 9002",
    "wait-for-backend": "node scripts/wait-for-backend.js",
    "build": "vite build",
    "preview": "vite preview --host ${HOST:-0.0.0.0} --port ${PORT:-4173}",
    "clean": "pkill -f 'python.*app|vite' || true",
    "dev:backend": "cd backend && source venv/bin/activate && export PYTHONPATH=$PYTHONPATH:$(dirname $(pwd)) && BACKEND_PORT=5001 python3 app.py",
    "dev:frontend": "vite --host 0.0.0.0 --port 9002",
    "check:ts-prune": "ts-prune",
    "check:depcheck": "depcheck --skip-missing true --ignore-dirs=backend,dist --ignores=@types/node,typescript,vite,@vitejs/plugin-react,tailwindcss,@tailwindcss/postcss,postcss,autoprefixer",
    "check:js": "npm-run-all -p check:ts-prune check:depcheck"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "11.11.4",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.1.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "depcheck": "^1.4.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "ts-prune": "^0.10.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  }
}
